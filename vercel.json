{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "assets/**",
      "use": "@vercel/public"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    },
    {
      "src": "/assets",
      "dest": "./public"
    }
  ]
}
